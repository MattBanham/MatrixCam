{
  "name": "matrix-cam-standalone",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4173 --strictPort",
    "build": "vite build",
    "preview": "vite preview --port 4173 --strictPort",
    "electron": "npm run build && electron electron/main.cjs",
    "electron:dev": "concurrently -k \"vite --port 4173 --strictPort\" \"wait-on tcp:4173 && VITE_DEV_SERVER_URL=http://localhost:4173 electron electron/main.cjs\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.0",
    "concurrently": "^9.0.1",
    "electron": "^33.2.1",
    "typescript": "^5.5.4",
    "vite": "^7.3.1",
    "wait-on": "^8.0.1"
  }
}
